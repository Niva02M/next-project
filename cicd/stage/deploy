#!/bin/bash

set -e

aws ecr get-login-password --region ap-southeast-2 | docker login --username AWS --password-stdin 235494784270.dkr.ecr.ap-southeast-2.amazonaws.com

cd /home/ebpearls/stage
docker-compose pull web_stage
docker-compose rm -s -f web_stage
docker-compose up -d web_stage
docker system prune -f

